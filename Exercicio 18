#include <stdio.h>
#include <string.h>

void bubbleSort(char vet[10][20]){
    int i, j;
    char tmp[20];
    int flag;
    for(i=0; i<9; i++){
        flag = 1; 
        for(j=0; j < (10-i-1); j++){
            if(strcmp(vet[j], vet[j+1]) > 0){
                strcpy(tmp, vet[j]);
                strcpy(vet[j], vet[j+1]);
                strcpy(vet[j+1], tmp);
                flag = 0;
            }
        }
        if(flag == 1){
            return;
        }
    }
}

void print(char vet[10][20]){
	int i;
	for(i=0; i<10; i++){
		printf("Element %d: %s\n", i+1, vet[i]);
	}
}

int main(){
	
	char vet[10][20];
	strcpy(vet[0], "Zilma");
	strcpy(vet[1], "Xuxa");
	strcpy(vet[2], "Pedro");
	strcpy(vet[3], "Kesia");
	strcpy(vet[4], "Mateus");
	strcpy(vet[5], "Neymar");
	strcpy(vet[6], "Junior");
	strcpy(vet[7], "Eliones");
	strcpy(vet[8], "Brendda");
	strcpy(vet[9], "Isabel");
	
	printf("----A lista desordenada e: -----\n\n");
	print(vet);
	bubbleSort(vet);
	printf("\n----A lista ordenada e: ----\n\n");
	print(vet);
	return 0;
}
